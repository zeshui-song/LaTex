    "#Inputs\n",
    "M = 0.1 #kg\n",
    "m = 0.302 #kg\n",
    "y = 0.17 #m\n",
    "\n",
    "#Calculations\n",
    "mu_s = M/m * 1.07 / np.sqrt(1.07**2 - y**2)\n",
    "\n",
    "#Print result\n",
    "print(f\"Static coefficient of friction = {mu_s:.4f}\")"
